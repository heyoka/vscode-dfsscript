{
	"scopeName": "source.dfs",
	"name": "DFSscript",
	"comment": "DFSscript language",
	"fileTypes": [
		"dfs"
	],
	"patterns": [{
        "comment": "Additional keywords",
		"match": "^def\\s|(?<=\\()lambda(?=:\\s)||",
		"name": "keyword.other"
	},{
        "comment": "Global Variables",
		"match": "stream|batch|influxql",
		"name": "support.variable"
	},{
    "comment": "Bools",
		"match": "TRUE|FALSE|true|false",
		"name": "support.variable"
	},{
        "comment": "Operators",
		"match": "AND|OR|&&|\\|\\|",
		"name": "support.variable"
	},{
        "comment": "Global Variables",
		"match": "TODO.*$|FIXME.*$|XXX.*$|BUG.*$",
		"name": "punctuation.definition.comment"
	},{
        "comment": "Single quoted string",
		"match": "'[^'].*'",
		"name": "string.single"
	},{
        "comment": "Double quoted string",
		"match": "\".*\"",
		"name": "string.double"
	},{
		"comment": "text string",
		"match": "<<<.*>>>",
		"name": "string.text"
	},{
        "comment": "Time duration",
		"match": "\\d+ms|\\d+[uÂµmshdw]",
		"name": "constant.numeric"
	},{
        "comment": "Float",
		"match": "\\d+\\.\\d+",
		"name": "constant.numeric"
	},{
        "comment": "Integer",
		"match": "\\d+",
		"name": "constant.numeric"
	},{
    "comment": "Comments",
		"match": "%.*$",
		"name": "comment.line.percent"
	},{
        "comment": "Special Characters",
		"match": "\\.\\w+|\\|\\w+|@\\w+",
		"name": "constant.numeric"
	}
    ]
}


